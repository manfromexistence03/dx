{
  "name": "ustudy",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "manofexistence",
    "url": "https://twitter.com/manofexistence1"
  },
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:cli": "turbo --filter=shadcn-ui build",
    "build:registry": "bun--filter=www build:registry",
    "dev": "turbo run dev --parallel",
    "cli:dev": "turbo --filter=shadcn-ui dev",
    "cli:start": "bun--filter=shadcn-ui start:dev",
    "www:dev": "bun--filter=www dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "preview": "turbo run preview",
    "typecheck": "turbo run typecheck",
    "format:write": "turbo run format:write",
    "format:check": "turbo run format:check",
    "sync:templates": "./scripts/sync-templates.sh \"templates/*\"",
    "check": "turbo lint typecheck format:check",
    "release": "changeset version",
    "pub:beta": "cd packages/cli && bunpub:beta",
    "pub:release": "cd packages/cli && bunpub:release",
    "test": "turbo run test --force"
  },
  "dependencies": {
    "@babel/core": "^7.22.1",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@clerk/elements": "^0.4.5",
    "@clerk/nextjs": "^5.0.12",
    "@clerk/themes": "^2.1.6",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@manypkg/cli": "^0.20.0",
    "@nextui-org/react": "^2.4.0",
    "@types/canvas-confetti": "^1.6.4",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/parser": "^5.59.7",
    "@udecode/plate-paragraph": "^33.0.0",
    "autoprefixer": "^10.4.14",
    "canvas-confetti": "^1.9.3",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "emblor": "^1.0.3",
    "eslint": "^8.41.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-turbo": "^1.9.9",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.12.0",
    "firebase": "^10.12.0",
    "framer-motion": "^11.1.7",
    "husky": "^8.0.3",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "react-syntax-highlighter": "^15.5.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.5",
    "tsx": "^4.1.4",
    "turbo": "^1.9.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.27",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.22",
    "typescript": "^5.4.2"
  }
}

    "ink": "^4.4.1",
    "ink-table": "^3.1.0",
    "inquirer": "^9.2.19",
    "log-symbols": "^6.0.0",
    "meow": "^13.2.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",

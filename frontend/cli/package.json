    "codecov": "^3.6.5",
    "commander": "^12.0.0",
    "coveralls": "^3.0.9",
    "drawille": "^2.0.2",
    "enquirer": "^2.4.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",

{
  "name": "dx-cli-nodejs",
  "private": false,
  "type": "module",
  "license": "MIT",
  "version": "0.0.1",
  "homepage": "https://github.com/dx-ui-org/dx-ui-cli#readme",
  "description": "A CLI tool that unlocks seamless ui frameworks integration",
  "keywords": [
    "UI",
    "CLI",
    "Tool",
    "dx-ui",
    "Template",
    "Integration",
    "Add Component"
  ],
  "author": {
    "name": "dx-ui",
    "email": "support@dx-ui.org",
    "url": "https://github.com/dx-ui-org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dx-ui-org/dx-ui-cli.git"
  },
  "bugs": {
    "url": "https://github.com/dx-ui-org/dx-ui-cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "dx-ui": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "link:cli": "pnpm link --global",
    "link:remove": "pnpm uninstall --global dx-ui-cli",
    "build": "tsup",
    "update:components": "tsx src/scripts/update/update-components.ts",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . -max-warnings=0 --fix",
    "check:prettier": "prettier --check .",
    "check:types": "tsc --noEmit",
    "changelog": "npx conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . && git add .",
    "release": "bumpp --execute='pnpm run changelog'",
    "postbuild": "pnpm run update:components",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
